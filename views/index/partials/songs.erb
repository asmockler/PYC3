<% @favorites ||= [] %>
<% @songs.each do |song| %>
<a href="#" class="song just-loaded <% if @favorites.include? song.slug %>fav<% end %>" data-sc-url="<%= song.soundcloud_url %>" data-artist="<%= song.artist %>" data-title="<%= song.title %>" data-slug="<%= song.slug %>">
	<img src="<%= song.album_art %>" class="img-responsive">
	<div class="song-info">
		<p><%= song.title %></p>
		<p><%= song.artist %></p>
	</div>
	<div class="song-tools">
		<i class="fa fa-heart"></i>
	</div>
</a>
<% end %>